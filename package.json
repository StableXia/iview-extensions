{
  "name": "iview-extensions",
  "version": "1.0.2",
  "description": "iview组件库扩展",
  "main": "lib/iview-extensions.js",
  "scripts": {
    "dev": "webpack-dev-server --config build/webpack.dev.config.js & npm run format",
    "build": "node build/webpack.prod.config.js",
    "build-component": "node build/webpack.prod-component.config.js",
    "build-component-style": "gulp --gulpfile build/build-style.config.js",
    "build-component-locale": "node build/webpack.prod-locale.config.js",
    "format": "onchange 'src/' 'examples/' -- prettier --write {{changed}}",
    "prepublish": "npm run build-component & npm run build-component-style & npm run build-component-locale",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiaotangdou/iview-extensions.git"
  },
  "keywords": [
    "iview",
    "extensions"
  ],
  "author": "xw",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xiaotangdou/iview-extensions/issues"
  },
  "homepage": "https://github.com/xiaotangdou/iview-extensions#readme",
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "cli-color": "^1.3.0",
    "deepmerge": "^2.2.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "husky": "^3.0.3",
    "lint-staged": "^9.2.1",
    "node-sass": "^4.9.3",
    "onchange": "^6.0.0",
    "ora": "^3.0.0",
    "popper.js": "^0.6.4",
    "prettier": "^1.18.2",
    "script-loader": "^0.7.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "vue": "^2.5.2",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "clipboard": "^2.0.1",
    "copy-webpack-plugin": "^4.5.1",
    "core-js": "^2.5.7",
    "css-loader": "^3.2.0",
    "echarts": "^4.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-clean-css": "^3.10.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.1",
    "gulp-scss": "^1.4.0",
    "highlightjs": "^9.10.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "portfinder": "^1.0.13",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue-loader": "^14.2.2",
    "vue-style-loader": "^4.1.0",
    "vuex": "^3.0.1",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "eslint --fix --ignore-pattern",
      "git add"
    ]
  }
}
